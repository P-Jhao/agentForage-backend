{
  "name": "agentforge-backend",
  "version": "1.0.0",
  "description": "AgentForge - 多功能 AI Agent 平台后端服务",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "init-db": "tsx src/scripts/init-db.ts",
    "clear": "tsx src/scripts/fix-duplicate-indexes.ts",
    "lint": "eslint src --fix",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "agent",
    "ai",
    "llm",
    "koa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@koa/bodyparser": "^5.1.0",
    "@koa/cors": "^5.0.0",
    "@koa/multer": "^4.0.0",
    "@koa/router": "^15.1.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "agentforge-gateway": "workspace:*",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^3.1.1",
    "koa-mount": "^4.2.0",
    "koa-static": "^5.0.0",
    "multer": "^2.0.2",
    "mysql2": "^3.9.0",
    "sequelize": "^6.37.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.15.0",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-static": "^4.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__multer": "^2.0.8",
    "@types/koa__router": "^12.0.4",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "eslint": "^9.28.0",
    "fast-check": "^4.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.34.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
